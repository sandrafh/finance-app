{
  "expo": {
    "name": "finance-app",
    "slug": "finance-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/logo/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "entryPoint": "./src/App.tsx",
    "splash": {
      "image": "./src/assets/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#b8dfda"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sandraflores.financeapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "4"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/logo.png",
        "backgroundColor": "#b8dfda"
      },
      "package": "com.sandraflores.financeapp",
      "versionCode": 4
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "010f0d53-3b13-4d50-a105-e0fe767d7ea3"
      }
    }
  }
}